{
  "name": "@gerhobbelt/markdown-it-chain",
  "version": "1.3.0-1",
  "description": "A chaining API like webpack-chain but for markdown-it.",
  "repository": {
    "url": "ULIVZ/markdown-it-chain",
    "type": "git"
  },
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "xo src",
    "fix": "xo --fix src",
    "jest": "jest",
    "release": "release-it --dry-run",
    "pub": "npm publish --access public"
  },
  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "8"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "webpack-chain": "^6.4.0"
  },
  "keywords": [
    "markdown-it",
    "chain"
  ],
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@gerhobbelt/markdown-it": "10.0.0-31",
    "@gerhobbelt/markdown-it-anchor": "^4.0.0-11",
    "@gerhobbelt/markdown-it-table-of-contents": "^0.4.3-1",
    "conventional-changelog-cli": "2.0.31",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-vue-libs": "4.0.0",
    "jest-cli": "25.2.7",
    "release-it": "13.5.1",
    "xo": "0.28.3"
  },
  "peerDependencies": {
    "@gerhobbelt/markdown-it": ">=10"
  },
  "xo": {
    "extends": [
      "plugin:vue-libs/recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "unicorn/filename-case": 0
    },
    "envs": [
      "jest"
    ]
  }
}
